{
  "name": "misty-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"yarn watch:server\" \"yarn watch:client\"",
    "watch:server": "cross-env NODE_ENV=development nodemon --watch './**/*.ts' --exec 'ts-node' server/index.ts",
    "watch:client": "./node_modules/.bin/webpack --watch",
    "start": "cross-env NODE_ENV=production ts-node dist/server/index.js",
    "build": "concurrently \"yarn build:server\" \"yarn build:client\"",
    "build:server": "tsc -p server",
    "build:client": "./node_modules/.bin/webpack"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "pug": "^3.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/webpack-env": "^1.16.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.0.2",
    "cross-env": "^7.0.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "html-webpack-pug-plugin": "^3.0.0",
    "nodemon": "^2.0.7",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.1.0",
    "ts-node": "^9.1.1",
    "webpack": "^5.35.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-hot-middleware": "^2.25.0"
  }
}
